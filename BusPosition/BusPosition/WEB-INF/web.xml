        <servlet-name>aggiungiCorsa</servlet-name>
        <url-pattern>/aggiungiCorsa</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>rimuoviCorsa</servlet-name>
        <servlet-class>delbello.rimuoviCorsa</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rimuoviCorsa</servlet-name>
        <url-pattern>/rimuoviCorsa</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>scegliLinea</servlet-name>
